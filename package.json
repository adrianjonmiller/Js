{
  "name": "Js",
  "version": "0.1.1",
  "description": "Produce universal library with webpack and es6",
  "main": "dist/Js.min.js",
  "scripts": {
    "build": "webpack --env build",
    "demo": "webpack --progress --colors --watch --env demo",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "browser-sync": "2.23.6",
    "browser-sync-webpack-plugin": "2.0.1",
    "chai": "3.5.0",
    "css-loader": "0.28.10",
    "eslint": "^3.19.0",
    "eslint-loader": "1.7.1",
    "mocha": "3.3.0",
    "node-sass": "4.9.3",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.3",
    "webpack": "2.4.1",
    "yargs": "7.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krasimir/webpack-library-starter.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Krasimir Tsonev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krasimir/webpack-library-starter/issues"
  },
  "homepage": "https://github.com/krasimir/webpack-library-starter",
  "dependencies": {
    "handlebars": "4.0.11",
    "handlebars-loader": "1.6.0",
    "lodash": "4.17.4"
  }
}
